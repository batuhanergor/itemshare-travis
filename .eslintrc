{
  "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "always"],
    "react/prop-types": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": { "browser": true }
}
